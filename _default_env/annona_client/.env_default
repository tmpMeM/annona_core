TZ=Asia/Shanghai
#TZ=UTC
#
#日志级别  panic fatal error warn info debug trace
LOG_LEVEL=debug
#
# client 登录验证通知机器人 token https://t.me/gcsgjcjk_bot
APP_TELEGRAM_BOT_TOKEN=*
# APP API id
APP_TELEGRAM_API_ID=*
# APP API Hash
APP_TELEGRAM_API_HASH=*
# server db redis address
SERVER_DB_REDIS_ADDRESS=host.docker.internal:6379
#REDIS password
SERVER_DB_REDIS_PASSWORD=db_password
