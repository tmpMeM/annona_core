CORE_API_URL=https://core.server.domain
#CORE_API_URL=http://host.docker.internal:8080
TDLIB_SIMPLE_MESSAGE_ENABLE=true
#屏蔽长文本
BLOCK_LONG_TEXT_ENABLE=true
#屏蔽长文本，最大字数
BLOCK_LONG_TEXT_MAX_COUNT=100