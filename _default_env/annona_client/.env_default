NOTICE_OF_FEEDRICHMSG_PUSH_URL_PATH=/ws/push_v2/
#
#
#平台类型 tele
#PLATFORM_TYPE_TELE
PLATFORM_TYPE_TELE=tele
# core api 验证token core_api_token
CORE_API_TOKEN=db_annona
